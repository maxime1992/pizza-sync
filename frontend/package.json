{
  "name": "pizza-sync",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "ng build --prod --e=prod --no-sourcemap"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.1.1",
    "@angular/common": "4.1.1",
    "@angular/compiler": "4.1.1",
    "@angular/core": "4.1.1",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "4.1.1",
    "@angular/http": "4.1.1",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "4.1.1",
    "@angular/platform-browser-dynamic": "4.1.1",
    "@angular/router": "4.1.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.3",
    "@ngrx/store": "2.2.2",
    "@ngrx/store-devtools": "3.2.4",
    "angular2-prettyjson": "2.0.5",
    "core-js": "2.4.1",
    "countdown": "2.6.0",
    "csv-file-creator": "1.0.7",
    "hammerjs": "2.0.8",
    "material-design-icons-iconfont": "3.0.3",
    "ng2-translate": "5.0.0",
    "ng2-webstorage": "1.5.1",
    "ngrx-store-freeze": "0.1.9",
    "redux": "3.6.0",
    "redux-batched-actions": "0.2.0",
    "rxjs": "5.3.1",
    "socket.io-client": "1.7.3",
    "zone.js": "0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.1.1",
    "@types/jasmine": "2.5.45",
    "@types/jest": "19.2.3",
    "@types/node": "7.0.8",
    "@types/socket.io-client": "1.4.29",
    "codelyzer": "3.0.0-beta.3",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "4.1.0",
    "jest": "19.0.2",
    "jest-preset-angular": "1.2.1",
    "protractor": "5.1.1",
    "ts-node": "3.0.4",
    "tslint": "4.5.1",
    "typescript": "2.3.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-jest.ts",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/environment/**",
      "!src/**/*.mock.ts",
      "!src/mocks/**",
      "!src/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/environment",
      "<rootDir>/src/mocks"
    ]
  }
}
